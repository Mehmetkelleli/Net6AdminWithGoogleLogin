@using Newtonsoft.Json
@using static Ui.Controllers.BaseController
<!DOCTYPE html>
<html class="loading light-layout dark-layout" lang="en" data-textdirection="ltr">
<!-- BEGIN: Head-->

<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
